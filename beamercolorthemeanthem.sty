\mode<presentation>

% Settings

%\colorlet{teal}{blue!50!green}
\definecolor{navyblue}{RGB}{0,77,94}
\definecolor{beige}{RGB}{250,240,200}
\colorlet{maindark}{navyblue}
\colorlet{mainlight}{white}

\setbeamercolor*{background canvas}{fg=maindark, bg=mainlight}
\setbeamercolor*{title page header}{fg=white}
\setbeamercolor*{subtitle}{fg=white}
\setbeamercolor*{author}{fg=maindark!70!black}
\setbeamercolor*{date}{fg=maindark!70!black}
\setbeamercolor*{frametitle}{fg=white}
\setbeamercolor*{framesubtitle}{fg=white}
\setbeamercolor*{page number in foot}{fg=white}
\setbeamercolor*{block title}{fg=white}
\setbeamercolor*{part title}{fg=maindark!70!black}

\setbeamercolor*{itemize item}{fg=maindark}
\setbeamercolor*{itemize subitem}{fg=maindark!80!black}
\setbeamercolor*{itemize subsubitem}{fg=maindark!80!white}

\setbeamercolor*{enumerate item}{fg=maindark}
\setbeamercolor*{enumerate subitem}{fg=maindark}
\setbeamercolor*{enumerate subsubitem}{fg=maindark}

\setbeamercolor{alerted text}{fg=red!70!black}
\setbeamercolor{normal text}{fg=mainlight!15!black}

\mode
<all>